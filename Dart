// mobile_money_service.dart
Future<bool> withdraw(double amount) async {
  final encryptedRequest = _encryptData({
    'amount': amount,
    'timestamp': DateTime.now().millisecondsSinceEpoch
  });
  
  // Utilisez une URL d'API protégée
  const apiUrl = String.fromEnvironment('MM_API_URL');
  final response = await http.post(
    Uri.parse('$apiUrl/withdraw'),
    headers: {
      'X-Encryption-Key': const String.fromEnvironment('ENCRYPTION_KEY'),
    },
    body: encryptedRequest
  );
  
  return response.statusCode == 202;
}
